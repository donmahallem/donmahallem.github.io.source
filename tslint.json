{
  "defaultSeverity": "error",
  "extends": [
    "@donmahallem/tslint-config"
  ],
  "rules": {
    "file-header": [
      true,
      {
        "match": "Source https://github.com/donmahallem/donmahallem.github.io.source",
        "allow-single-line-comments": true,
        "default": "Source https://github.com/donmahallem/donmahallem.github.io.source",
        "enforce-trailing-newline": false
      }
    ]
  },
  "rulesDirectory": [
    "codelyzer"
  ]
}
