{
  "name": "@donmahallem/github-page",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test:watch": "ng test",
    "lint": "ng lint && prettier --config ./.prettierrc src/**/*.ts --check",
    "lint:fix": "ng lint --fix && prettier --config ./.prettierrc src/**/*.ts --write",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.json -d ./docs",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "docs": "npm run compodoc",
    "prerender": "ng run DonMahallem:prerender",
    "dev:ssr": "ng run DonMahallem:serve-ssr",
    "serve:ssr": "node dist/DonMahallem/server/main.js",
    "build:ssr": "ng build --prod && ng run DonMahallem:server:production",
    "prepare:prerender": "node --loader ts-node/esm  ./prerender/create-prerender2.ts",
    "all": "npm run build && npm run lint && npm run docs && npm run prepare:prerender"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/donmahallem/donmahallem.github.io.source",
